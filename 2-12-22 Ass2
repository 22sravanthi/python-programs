
###Way-1

def eligible(age):
    if age>18:
        print('Yes')
    else:
        print('No')
age=int(input('enter age:'))
eligible(age)



###Way-2

def eligible(age):
    if age>18:
        return 'Yes'
    else:
        return 'No'
age=int(input('enter age:'))
print(eligible(age))



### way-3
    
def eligible(age):
    if age>18:
         return True
    else:
        return False
age=int(input('enter age:'))
if(eligible(age)):
    print('Yes')
else:
    print('No')
